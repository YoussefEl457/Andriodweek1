<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.quizapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="30" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/Theme.QuizApp" >
        <activity
            android:name="com.example.quizapp.QuizUitslag"
            android:screenOrientation="portrait"
            android:theme="@style/Fullscreen" />
        <activity
            android:name="com.example.quizapp.MuziekQuiz"
            android:screenOrientation="portrait"
            android:theme="@style/Fullscreen" />
        <activity
            android:name="com.example.quizapp.GamesQuiz"
            android:screenOrientation="portrait"
            android:theme="@style/Fullscreen" />
        <activity
            android:name="com.example.quizapp.GeografieQuiz"
            android:screenOrientation="portrait"
            android:theme="@style/Fullscreen" />
        <activity
            android:name="com.example.quizapp.QuizKeuze"
            android:screenOrientation="portrait"
            android:theme="@style/Fullscreen" />
        <activity
            android:name="com.example.quizapp.MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Fullscreen" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>